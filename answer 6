# yo baby abra ka dabra

d = int(input('Input distance (meters): '))
h = int(input('Input time (hour): '))
m = int(input('Input time(minutes): '))
s = int(input('Input time(seconds): '))

time = s + m*60 + h*60*60
print('Your speed in meters/sec is', d/time)

d = d/1000
time = time / 3600
print('Your speed in km/h is', d/time)

d = d/1.6
print('Your speed in miles/h is', d/time)
